    function Submit_contactHeaderForm()
{
    var ValidCap=ValidCaptchaHeader();
    var name=trim($("#headName").val());
    var orgName=$("#headOrgName").val();
    var email=$("#headEmailId").val();
    var phone=$("#headMobileNo").val();
    var address=$("#headAddress").val();
    var city=$("#selectCityInner").val();
    var state=$("#selectStateValueInner").val(); 
    var country=$("#selectCountryValueInner").val();
    var product=$("#selectProdtinner").val();//$("#product").val();
    var solution=$("#selectSolnInner").val(); //alert(solution);die;
    var message=$("#headMsg").val();
    var dataString;
    
    if(name=="")
    {
        $("#headName").addClass('error_msg1');
        $('#headName').attr({'placeholder': 'Please enter your Name'});
        $("#headName").keypress(function (){$("#headName").removeClass('error_msg1')});
        $("#headName").focus();
        return false;
    }
     else if(orgName=="")
    { 
        $("#headOrgName").addClass('error_msg1');
        $('#headOrgName').attr({'placeholder': 'Please enter your Organization Name'});
        $("#headOrgName").keypress(function (){$("#headOrgName").removeClass('error_msg1')});
        $("#headOrgName").focus();
        return false;
    }
    else if(email==""|| email==null)
    {
        $("#headName").removeClass('error_msg1');
        $("#headEmailId").addClass('error_msg1');
        $('#headEmailId').attr({'placeholder': 'Please enter your Email Id'});
        $("#headEmailId").keypress(function (){$("#headEmailId").removeClass('error_msg1')});
        $("#headEmailId").focus();
        return false;
    }
    else if(isEMail(email))
    {
        $("#headName").removeClass('error_msg1');
        $("#headEmailId").addClass('error_msg1');
        $('#headEmailId').attr({'placeholder': 'Please enter valid Email Id'});
        $("#headEmailId").keypress(function (){$("#headEmailId").removeClass('error_msg1')});
        $("#headEmailId").focus();
        $("#headEmailId").val('');
        return false;
    }
    
    else if(phone=="" || phone==null)
        {
        $("#headEmailId").removeClass('error_msg1');
        $("#headMobileNo").addClass('error_msg1');
        $('#headMobileNo').attr({'placeholder': 'Please enter your Phone No.'});
        $("#headMobileNo").keypress(function (){$("#headMobileNo").removeClass('error_msg1')});
        $("#headMobileNo").focus();
        return false;
        }
        else  if(!IsValid(phone))
        {
        $("#headEmailId").removeClass('error_msg1');
        $("#headMobileNo").addClass('error_msg1');
        $('#headMobileNo').attr({'placeholder': 'Please enter your Phone No.'});
        $("#headMobileNo").keypress(function (){$("#headMobileNo").removeClass('error_msg1')});
        $("#headMobileNo").focus();
        $("#headMobileNo").val('');
        return false;
        }
        else  if(phone.length<=9)
        {
        $("#headEmailId").removeClass('error_msg1');
        $("#headMobileNo").addClass('error_msg1');
        $('#headMobileNo').attr({'placeholder': 'Please enter your Phone No.'});
        $("#headMobileNo").keypress(function (){$("#headMobileNo").removeClass('error_msg1')});
        $("#headMobileNo").focus();
        $("#headMobileNo").val('');
        return false;
        }
//        else if( $("#clientCountry").val()=="0" || $("#clientCountry").val()=="" || $("#clientCountry").val()==null){
//     // else if (clientCountry == "" || clientCountry == null)
//        $("#clientCountry").focus();
//        $("#clientCountry").addClass("error_msg1");
//        $("#clientCountry").attr('placeholder', 'select Country');
//        setTimeout(function () {
//            $("#clientCountry").removeClass("error_msg1");
//        }, 3000);
//          str_return = "false";
//        return false;
//    }

//  Added by priyanka 7feb 2018(state, city and solution validation)
     else if (state == '0'){  //alert('state');die;
        $("#selectStateValueInner").focus();
        $("#selectStateValueInner").addClass("error_msg1");
        $("#selectStateValueInner").attr('placeholder', 'select State');
        $("#selectStateValueInner").change(function () {
            $("#selectStateValueInner").removeClass('error_msg1')
        });
        $("#selectStateValueInner").focus();
        return false;
     }
     else if (city == "" || city == null || city=="select")    {//alert('fgf');
        $("#selectCityInner").removeClass('error_msg1');
        $("#selectCityInner").addClass("error_msg1");
        $("#selectCityInner").attr({'placeholder': 'Please select city'});
        $("#selectCityInner").change(function () {
            $("#selectCityInner").removeClass('error_msg1')
        });
        $("#selectCityInner").focus();
        return false; 
    }  
        else if ($("#selectSolnInner").val()=="Select Solution*" ||$("#selectSolnInner").val()==" "||$("#selectSolnInner").val()=="null" ){
        $("#selectSolnInner").focus();
        $("#selectSolnInner").addClass("error_msg1");
        $("#selectSolnInner").attr('placeholder', 'select Solution');
        setTimeout(function () {
         $("#selectSolnInner").removeClass("error_msg1");
        }, 3000);
        str_return = "false";
        return false;
     }   
        
        else if($("#headInputCode").val()=='')
        {
        $("#headMobileNo").removeClass('error_msg1');
        $("#headInputCode").addClass('error_msg1');
        $('#headInputCode').attr({'placeholder': 'Type Letter'});
        $("#headInputCode").keypress(function (){$("#headInputCode").removeClass('error_msg1')});
        $("#headInputCode").focus();
        $("#headInputCode").val('');
            return false
        }
        else if(ValidCap==false)
        {
                 $("#headMobileNo").removeClass('error_msg1');
        $("#headInputCode").addClass('error_msg1');
        $('#headInputCode').attr({'placeholder': 'Letter Not Mached'});
        $("#headInputCode").keypress(function (){$("#headInputCode").removeClass('error_msg1')});
        $("#headInputCode").focus();
        $("#headInputCode").val('');
            return false;
        }
          else{ 
        dataString = 'name='+ name + '&orgName=' + orgName +'&email=' + email + '&address=' + address+ '&phone=' + phone+ '&city=' + city+ '&state=' +state+ '&country=' + country + '&solution=' + solution+ '&message=' + message;
        //dataString = 'name='+ name + '&orgName=' + orgName +'&email=' + email + '&address=' + address+ '&phone=' + phone+ '&city=' + city+ '&state=' + state+ '&country=' + country + '&product=' + product+ '&solution=' + solution+ '&message=' + message;
    
        $.ajax({
            type: "POST",
              url: path+"contactSubmit.php",
            data: dataString,
            async: false,
            success: function(result) { 
                   location.href=path+"thankyou.php"; 
//                alert('Message has been Send');
//                $("#headName").val('');
//                $("#headOrgName").val('');
//                $("#headEmailId").val('');
//                $("#headMobileNo").val('');
//                $("#headAddress").val('');
//                $(".city").val('');
//                $("#selectProdt").val('');
//                $("#selectSoln").val('');
//                $("#headMsg").val('');
//                $("#headInputCode").val('');
                gettouch_hide();
            },error: function (result,err,status) {
                 alert(err+'-----'+status);
                alert('Please Submit Form again');
            }
        });}
    }

  function ValidCaptchaContact(){
                     var string1 = removeSpaces($('#mainCaptchaCode').val());
                     var string2 = $("#txtInputcode").val();
      if (string1 == string2){
                        return true;
                      }
                      else{        
                        return false;
                      }
                  }
  function ValidCaptchaHeader(){
   //  alert($('#headMainCaptcha').val());
     //alert($("#headInputCode").val());
     //return false;
                     var string1 = removeSpacesHeader($('#headMainCaptcha').val());
                     var string2 = removeSpacesHeader($("#headInputCode").val());
                      

      if (string1 == string2){
                        return true;
                      }
                      else{        
                        return false;
                      }
                  }
  function CaptchaContact() {
    $("#txtInputcode").val('');
//    var alpha = new Array('A', 'B', 'C', 'D', 'E', 'F', 'G', 'H', 'I', 'J', 'K', '1', '2', '3', 'L', 'M', 'N', 'O', 'P', 'Q', 'R', 'S', 'T', '5', '8', '3', 'U', 'V', 'W', 'X', 'Y', 'Z', '7', '8', '9', '0', 'a', 'b', 'c', 'd', 'e', 'f', 'g', 'h', '4', '5', '6', 'i', 'j', 'k', 'l', 'm', 'n', 'o', 'p', 'q', 'r', 's', 't', '1', '8', 'u', 'v', 'w', 'x', 'y', 'z');
    var alpha = new Array('a', 'b', 'c', 'd', 'e', 'f', 'g', 'h','i', 'j', 'k', 'l', 'm', 'n', 'o', 'p', 'q', 'r', 's', 't', 'u', 'v', 'w', 'x', 'y', 'z');
    var i;
    for (i = 0; i < 6; i++) {
        var a = alpha[Math.floor(Math.random() * alpha.length)];
        var b = alpha[Math.floor(Math.random() * alpha.length)];
        var c = alpha[Math.floor(Math.random() * alpha.length)];
        var d = alpha[Math.floor(Math.random() * alpha.length)];
        var e = alpha[Math.floor(Math.random() * alpha.length)];
        var f = alpha[Math.floor(Math.random() * alpha.length)];
        var g = alpha[Math.floor(Math.random() * alpha.length)];
    }
    var code = a + ' ' + b + ' ' + ' ' + c + ' ' + d + ' ' + e + ' ' + f + ' ' + g;
   // alert(code);
    $("#mainCaptchaCode").val(code);
}

function CaptchaHeader() {
    $("#headInputCode").val('');
    var alpha = new Array('a', 'b', 'c', 'd', 'e', 'f', 'g', 'h','i', 'j', 'k', 'l', 'm', 'n', 'o', 'p', 'q', 'r', 's', 't', 'u', 'v', 'w', 'x', 'y', 'z');
    //var alpha = new Array('A', 'B', 'C', 'D', 'E', 'F', 'G', 'H', 'I', 'J', 'K', '1', '2', '3', 'L', 'M', 'N', 'O', 'P', 'Q', 'R', 'S', 'T', '5', '8', '3', 'U', 'V', 'W', 'X', 'Y', 'Z', '7', '8', '9', '0', 'a', 'b', 'c', 'd', 'e', 'f', 'g', 'h', '4', '5', '6', 'i', 'j', 'k', 'l', 'm', 'n', 'o', 'p', 'q', 'r', 's', 't', '1', '8', 'u', 'v', 'w', 'x', 'y', 'z');
    var i;
    for (i = 0; i < 6; i++) {
        var a = alpha[Math.floor(Math.random() * alpha.length)];
        var b = alpha[Math.floor(Math.random() * alpha.length)];
        var c = alpha[Math.floor(Math.random() * alpha.length)];
        var d = alpha[Math.floor(Math.random() * alpha.length)];
        var e = alpha[Math.floor(Math.random() * alpha.length)];
        var f = alpha[Math.floor(Math.random() * alpha.length)];
        var g = alpha[Math.floor(Math.random() * alpha.length)];
    }
    var code = a + ' ' + b + ' ' + ' ' + c + ' ' + d + ' ' + e + ' ' + f + ' ' + g;
    
    $("#headMainCaptcha").val(code);
}

function removeSpaces(string) {
    return string.split(' ').join('');
}
function removeSpacesHeader(string1) {
    
    return string1.split(' ').join('');
}
// client Pay 
function clientPaySubmit()
{
    var ValidCap       = ValidCaptchaContact();
    var clientName     = trim($("#clientName").val());
    var clientOrgName  = trim($("#clientOrgName").val());
    var clientEmailId  = $("#clientEmailId").val();
    var clientMobileNo = $("#clientMobileNo").val();
    var clientAddress  = $("#clientAddress").val();
    var clientCountry  = $("#clientCountry").val();
    var clientState    = $("#clientState").val();
    var clientCity     = $("#clientCity").val();
    var amount         = $("#amount").val() ;
    var phoneNo        = $("#phoneNo").val() ;
    var zipCode        = $("#zipCode").val() ;
    var products       =  '';
    var salePerName    =  trim($("#salePerName").val());
    var radioValue = $("input[name='payementMode']:checked").val();
    radioValue = 'payU';
    $("#payMode").val(radioValue) ;
if(radioValue == undefined){
      alert("Select the Payment Mode.")
        return false;
}else  if (clientName == "")
    {
       $("#clientName").addClass('error_msg1');
       $('#clientName').attr({'placeholder': 'Please enter your Name'}); 
       $("#clientName").keypress(function () {
            $("#clientName").removeClass('error_msg1')
        });
        $("#clientName").focus();
        return false;
    }else if (clientOrgName == "" || clientOrgName == null)
    {
        $("#clientOrgName").removeClass('error_msg1');
        $("#clientOrgName").addClass('error_msg1');
        $('#clientOrgName').attr({'placeholder': 'Please enter your Organization Name'});
        $("#clientOrgName").keypress(function () {
            $("#clientOrgName").removeClass('error_msg1')
        });
        $("#clientOrgName").focus();
        return false;
    }else if (clientEmailId == "" || clientEmailId == null)
    {
        $("#clientEmailId").removeClass('error_msg1');
        $("#clientEmailId").addClass('error_msg1');
        $('#clientEmailId').attr({'placeholder': 'Please enter your Email Id'});
        $("#clientEmailId").keypress(function () {
            $("#clientEmailId").removeClass('error_msg1')
        });
        $("#clientEmailId").focus();
        return false;
    }
    else if (isEMail(clientEmailId))
    {
        $("#clientEmailId").removeClass('error_msg1');
        $("#clientEmailId").addClass('error_msg1');
        $('#clientEmailId').attr({'placeholder': 'Please enter valid Email Id'});
        $("#clientEmailId").keypress(function () {
            $("#clientEmailId").removeClass('error_msg1')
        });
        $("#clientEmailId").focus();
        $("#clientEmailId").val('');
        return false;
    }

    else if ( clientMobileNo == "" || clientMobileNo == null )
    {
        $("#clientMobileNo").removeClass('error_msg1');
        $("#clientMobileNo").addClass('error_msg1');
        $('#clientMobileNo').attr({'placeholder': 'Please enter your Mobile No.'});
        $("#clientMobileNo").keypress(function () {
            $("#clientMobileNo").removeClass('error_msg1')
        });
        $("#clientMobileNo").focus();
        return false;
    }else if (trim(clientMobileNo).length< 10 )
    {
        $("#clientMobileNo").removeClass('error_msg1');
        $("#clientMobileNo").addClass('error_msg1');
        $('#clientMobileNo').attr({'placeholder': 'Please enter valid Mobile No.'});
        $("#clientMobileNo").keypress(function () {
            $("#clientMobileNo").removeClass('error_msg1')
        });
        $("#clientMobileNo").focus();
        $("#clientMobileNo").val('');
        return false;
    }
    else if (!IsValid(clientMobileNo))
    {
        $("#clientMobileNo").removeClass('error_msg1');
        $("#clientMobileNo").addClass('error_msg1');
        $('#clientMobileNo').attr({'placeholder': 'Please enter valid Mobile No.'});
        $("#clientMobileNo").keypress(function () {
            $("#clientMobileNo").removeClass('error_msg1')
        });
        $("#clientMobileNo").focus();
        $("#clientMobileNo").val('');
        return false;
    }else if (clientAddress == "" || clientAddress == null)
    {
        $("#clientAddress").removeClass('error_msg1');
        $("#clientAddress").addClass("error_msg1");
        $("#clientAddress").attr({'placeholder': 'Please enter your Address'});
        $("#clientAddress").keypress(function () {
            $("#clientAddress").removeClass('error_msg1')
        });
        $("#clientAddress").focus();
        return false;
    }else if (clientCountry == "" || clientCountry == null)
    {
        $("#clientAddress").removeClass('error_msg1');
        $("#clientCountry").addClass("error_msg1");
        $("#clientCountry").change(function () {
            $("#clientCountry").removeClass('error_msg1')
        });
        $("#clientCountry").val("");
        $("#clientCountry").focus();
        return false;
    }else if (clientState == "" || clientState == null)
    {
        $("#clientCountry").removeClass('error_msg1');
        $("#clientState").addClass("error_msg1");
        $("#clientState").change(function () {
            $("#clientState").removeClass('error_msg1')
        });
        $("#clientState").val("");
        $("#clientState").focus();
        return false;
    }else if (clientCity == "" || clientCity == null)
    {
        $("#clientState").removeClass('error_msg1');
        $("#clientCity").addClass("error_msg1");
        $("#clientCity").attr({'placeholder': 'Please enter your Address'});
        $("#clientCity").change(function () {
            $("#clientCity").removeClass('error_msg1')
        });
        $("#clientCountry").focus();
        return false; 
    }else if ( zipCode == "" || zipCode == null)
    {
        $("#zipCode").removeClass('error_msg1');
        $("#zipCode").addClass('error_msg1');
        $('#zipCode').attr({'placeholder': 'Please enter your Pin code'});
        $("#zipCode").keypress(function () {
            $("#zipCode").removeClass('error_msg1')
        });
        $("#zipCode").focus();
        return false;
    }else if( $('input[type=checkbox]:checked').length == 0)
    {
        $("#productErr").text("Please choose at least One Product!!!");   
        return false;
    }else if( $('#checkbox-1-15').is(":checked") &&  trim($("#order").val())=='')
    {
        $("#order").addClass('error_msg1');
        $('#order').attr({'placeholder': 'Please enter Product Name'});
        $("#order").keypress(function () {
            $("#order").removeClass('error_msg1')
        });
        $("#order").focus();
        $("#order").val('');
        return false;
    }else if ( amount == '')
    {
        $("#amount").removeClass('error_msg1');
        $("#amount").addClass('error_msg1');
        $('#amount').attr({'placeholder': 'Enter Amount'});
        $("#amount").keypress(function () {
            $("#amount").removeClass('error_msg1')
        });
        $("#amount").focus();
        $("#amount").val('');
        return false
    }else if ( salePerName == "" || salePerName == null) 
    {
        $("#salePerName").removeClass('error_msg1');
        $("#salePerName").addClass('error_msg1');
        $('#salePerName').attr({'placeholder': "Please enter Sale's Person Name"});
        $("#salePerName").keypress(function () {
            $("#salePerName").removeClass('error_msg1')
        });
        $("#salePerName").focus();
        return false;
    }else if($("#txtInputcode").val()=='')
        {
        $("#salePerName").removeClass('error_msg1');
        $("#txtInputcode").addClass('error_msg1');
        $('#txtInputcode').attr({'placeholder': 'Type Captcha'});
        $("#txtInputcode").keypress(function (){$("#txtInputcode").removeClass('error_msg1')});
        $("#txtInputcode").focus();
        $("#txtInputcode").val('');
            return false
        }
    else if ( ValidCap == false )
    {
        $("#clientMobileNo").removeClass('error_msg1');
        $("#txtInputcode").addClass('error_msg1');
        $('#txtInputcode').attr({'placeholder': 'Captcha Not Matched'});
        $("#txtInputcode").keypress(function () {
            $("#txtInputcode").removeClass('error_msg1')
        });
        $("#txtInputcode").focus();
        $("#txtInputcode").val('');
        return false;
    }
    else {
            
            $("#dialogInner").show();
            $("#dialogOuter").show();
            // to show info on pop up
        var checkedBox = [];
          $('input[type=checkbox]:checked').each(function(i){
              if($(this).val()=="Other"){
                  checkedBox[i] = $("#order").val();
              }else{
                  checkedBox[i] = $(this).val();
              }
          
        });
        var checkedBoxVal = checkedBox.join(); 
            $("#allProNames").val(checkedBoxVal); // all the product selected by client
            
            $("#clientNamePop").text(clientName);
            $("#clientOrgaPop").text(clientOrgName);
            $("#clientEmailPop").text(clientEmailId);
            $("#clientMobPop").text(clientMobileNo);
            $("#clientAddPop").text(clientAddress);
            clientCountry  = clientCountry.split('_');
            $("#clientCountryPop").text(clientCountry[0]);
            $("#clientStatePop").text(clientState);
            $("#clientCityPop").text(clientCity);
            $("#clientPhonePop").text(phoneNo);
            $("#clientPinCodePop").text(zipCode);
            $("#clientProductPop").text(products);
            $("#clientAmountPop").text(amount);
            $("#clientProductPop").text(checkedBoxVal);
            $("#salePersonPop").text(salePerName);
        }
}

$("#clientPopCancel").click(function(){
            $("#dialogInner").fadeOut(1050);
            $("#dialogOuter").fadeOut(1000);
});

$("#clientPopOk").click(function(){
            $("#dialogInner").fadeOut(1050);
            $("#dialogOuter").fadeOut(1000);
            $("#submitCheck").val('1');
            $("#clientInfoForm").submit();
           
});
 function checkSubmit(){
     var check = $("#submitCheck").val();
     if(check=='1'){
         return true ;
     }else{
         return false ;
     }
 }
 
function trim(val){
    return val.replace(/^\s+|\s+$/g,'');
}

function isNumberDecimalKey(evt) {
    var charCode = (evt.which) ? evt.which : evt.keyCode;
    if (charCode == 46)
    {
        if (($("#amount").val()).length != 2) {
            return false ;
        }
        
    } else {
        if (charCode > 31 && (charCode != 46 && ( charCode != 37 && charCode != 39 && charCode < 48 || charCode > 57)))
        {
            return false;
        } else if (($("#amount").val()).length == 10 && (charCode != 37 && charCode != 39 && charCode != 46 && charCode != 8 && charCode != 9)) {
            return false;
        }
        else {
            return true;
        }
    }
}

function clientCountryChange(countrytText){
   
    $("#loaderOuter").css("display", "block");
    $("#loader").css("display", "block");
    var url = "mode=getClientState&countrytText="+countrytText ;
    $.ajax({
        type: 'post',
           url: path+"contactSubmit.php",
        data: url,
        error: function (jqXHR, textStatus, errorThrown) {

        },
        success: function (data) {
            $('#clientState').children().remove();
            $("#clientState").append(data);
            $("#loaderOuter").css("display", "none");
            $("#loader").css("display", "none");
        }

    });
}
function clientCityChange(stateId){
    $("#loaderOuter").css("display", "block");
    $("#loader").css("display", "block");
    var stateText  = $("#clientState option:selected").text();
    var url = "mode=getClientCity&stateId="+stateId+"&stateText="+stateText ;
    $.ajax({
        type: 'post',
        url: path+"contactSubmit.php",
        data: url,
        error: function (jqXHR, textStatus, errorThrown) {

        },
        success: function (data) {
            $('#clientCity').children().remove();
            $("#clientCity").append(data);
            $("#loaderOuter").css("display", "none");
            $("#loader").css("display", "none");
        }

    });
}
/*add by satyam*/
 function ScrollTo(name) {
  ScrollToResolver(document.getElementById(name));
}

function ScrollToResolver(elem) {
  var jump = parseInt(elem.getBoundingClientRect().top * .2);
    //alert(jump);
     if(jump >15){
      document.body.scrollTop += jump;
      document.documentElement.scrollTop += jump;
      }

 

  if (!elem.lastjump || elem.lastjump > Math.abs(jump)) {
    elem.lastjump = Math.abs(jump);
    setTimeout(function() { ScrollToResolver(elem);}, "100");
  } else {
    elem.lastjump = null;
  }
}

