                alert('Please Submit Form again');
            }
        });}
    }
function Submit_contactFormOuter(pagename=null)
{ 
    
    var ValidCap=ValidCaptchaContact();
    var name=trim($("#ctcName").val());
    var orgName=$("#ctcOrgName").val();
    var email=$("#ctcEmailId").val();
    var phone=$("#ctcMobileNo").val();
    var address=$("#ctcAddress").val();
    var city=$("#clientCity").val();
    var state=$("#clientState").val();
    var country=$("#clientCountry").val();
    var product=$("#selectProdtOuter").val();//$("#product").val();
    var solution=$("#selectSolnOuter").val();//$("#product").val();
    var message=$("#ctcMsg").val();
    var zc_gad=$("#zc_gad").val();
    if(zc_gad=="undefined"){
        zc_gad="";
    }

    var dataString;
    
    if(name=="")
    {   
        $("#ctcName").addClass('error_msg1');
        $('#ctcName').attr({'placeholder': 'Please enter your Name'});
        $("#ctcName").keypress(function (){$("#ctcName").removeClass('error_msg1')});
        $("#ctcName").focus();
        return false;
    }
    else if(!IsName($.trim($("#ctcName").val()))){
           
        $('#ctcName').val("");
        document.getElementById("ctcName").placeholder = "Please Enter Correct Details*";
        document.getElementById("ctcName").classList.add('error_msg1');
        $("#ctcName").focus();
        return false;
    } 
    
   else if(orgName=="")
    {
        $("#ctcOrgName").addClass('error_msg1');
        $('#ctcOrgName').attr({'placeholder': 'Please enter your Organization Name'});
        $("#ctcOrgName").keypress(function (){$("#ctcOrgName").removeClass('error_msg1')});
        $("#ctcOrgName").focus();
        return false;
    }
  
    else if(email==""|| email==null)
    {
        $("#ctcName").removeClass('error_msg1');
        $("#ctcEmailId").addClass('error_msg1');
        $('#ctcEmailId').attr({'placeholder': 'Please enter your Email Id'});
        $("#ctcEmailId").keypress(function (){$("#ctcEmailId").removeClass('error_msg1')});
        $("#ctcEmailId").focus();
        return false;
    }
    else if(isEMail(email))
    {
        $("#ctcName").removeClass('error_msg1');
        $("#ctcEmailId").addClass('error_msg1');
        $('#ctcEmailId').attr({'placeholder': 'Please enter valid Email Id'});
        $("#ctcEmailId").keypress(function (){$("#ctcEmailId").removeClass('error_msg1')});
        $("#ctcEmailId").focus();
        $("#ctcEmailId").val('');
        return false;
    }else if(phone=="" || phone==null)
        {
        $("#ctcEmailId").removeClass('error_msg1');
        $("#ctcMobileNo").addClass('error_msg1');
        $('#ctcMobileNo').attr({'placeholder': 'Please enter your Phone No.'});
        $("#ctcMobileNo").keypress(function (){$("#ctcMobileNo").removeClass('error_msg1')});
        $("#ctcMobileNo").focus();
        return false;
        }
        else  if(!IsValid(phone))
        {
        $("#ctcEmailId").removeClass('error_msg1');
        $("#ctcMobileNo").addClass('error_msg1');
        $('#ctcMobileNo').attr({'placeholder': 'Please enter your Phone No.'});
        $("#ctcMobileNo").keypress(function (){$("#ctcMobileNo").removeClass('error_msg1')});
        $("#ctcMobileNo").focus();
        $("#ctcMobileNo").val('');
        return false;
        }
        else  if(phone.length<=9)
        {
        $("#ctcEmailId").removeClass('error_msg1');
        $("#ctcMobileNo").addClass('error_msg1');
        $('#ctcMobileNo').attr({'placeholder': 'Please enter your Phone No.'});
        $("#ctcMobileNo").keypress(function (){$("#ctcMobileNo").removeClass('error_msg1')});
        $("#ctcMobileNo").focus();
        $("#ctcMobileNo").val('');
        return false;
        }
//         else if(address=="")
//    {
//        $("#ctcAddress").addClass('error_msg1');
//        $('#ctcAddress').attr({'placeholder': 'Please enter your Address'});
//        $("#ctcAddress").keypress(function (){$("#ctcAddress").removeClass('error_msg1')});
//        $("#ctcAddress").focus();
//        return false;
//    }
  else if( $("#clientCountry").val()=="0" || $("#clientCountry").val()=="" || $("#clientCountry").val()==null){
