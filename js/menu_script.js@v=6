        $(".txtCaptacha").keypress(function (){$(".txtCaptacha").removeClass('error_msg1')});
        $(".txtCaptacha").focus();
        $(".txtCaptacha").val('');
            return false;
        }
          else{

 // dataString = 'name='+ name + '&orgName=' + orgName +'&email=' + email + '&address=' + address+ '&phone=' + phone+ '&city=' + city+ '&state=' + state+ '&country=' + country + '&product=' + product+ '&solution=' + solution+ '&message=' + message;
        dataString = 'name='+ name + '&orgName=' + orgName +'&email=' + email + '&address=' + address+ '&phone=' + phone+ '&city=' + city+ '&state=' + state+ '&country=' + country + '&solution=' + solution+ '&message=' + message;
        $.ajax({
